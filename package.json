{
    "name": "fastify-react-boilerplate",
    "private": true,
    "scripts": {
        "build": "dotenv -e ./.env turbo build",
        "dev": "dotenv -e ./.env turbo dev",
        "lint": "turbo lint",
        "db": "dotenv -e ./.env turbo db -- --",
        "format": "prettier --write \"**/*.{ts,tsx,md}\""
    },
    "devDependencies": {
        "prettier": "^3.2.5",
        "turbo": "^2.0.4",
        "typescript": "^5.4.5"
    },
    "packageManager": "pnpm@9.1.4",
    "engines": {
        "node": ">=18"
    },
    "dependencies": {
        "dotenv-cli": "^7.4.2",
        "zod": "^3.23.8"
    }
}
